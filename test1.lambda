let fizz = n ->
  = 0 (% n 3)
in

let buzz = n ->
  = 0 (% n 5)
in

let fizzbuzz = n ->
  and (fizz n) (buzz n)
in

let range = fix (f, a, b ->
  if > a b
  then {}
  else (f a (- b 1)).concat {b})
in

(range 1 100).map (n ->
  if fizzbuzz n then "FizzBuzz"
  else if fizz n then "Fizz"
  else if buzz n then "Buzz"
  else n)
